# Use Git LFS for large image files
static/images/*.png filter=lfs diff=lfs merge=lfs -text
static/images/*.jpg filter=lfs diff=lfs merge=lfs -text
static/images/*.jpeg filter=lfs diff=lfs merge=lfs -text
static/images/*.gif filter=lfs diff=lfs merge=lfs -text
frontend/public/images/*.png filter=lfs diff=lfs merge=lfs -text
frontend/public/images/*.jpg filter=lfs diff=lfs merge=lfs -text
frontend/public/images/*.jpeg filter=lfs diff=lfs merge=lfs -text
frontend/public/images/*.gif filter=lfs diff=lfs merge=lfs -text
